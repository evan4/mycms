{
	"name": "ivan/mycms",
	"description": "php cms based on mvc pattern",
	"type": "project",
	"keywords": [
		"php cms"
	],
	"homepage": "https://github.com/evan4/mycms",
	"license": "MIT",
	"authors": [{
		"name": "Ivan Orlov",
		"email": "evan4mc@gmail.com",
		"role": "developer"
	}],
	"support": {
		"source": "https://github.com/evan4/mycms.git"
	},
	"require": {
		"php": "^7.1.3",
		"vlucas/phpdotenv": "^3.5"
	},
	"autoload": {
		"psr-4": {
			"Mycms\\": "core/",
			"App\\": "app/"
		},
		"files": []
	},
	"config": {
		"preferred-install": "dist",
		"sort-packages": true,
		"optimize-autoloader": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
